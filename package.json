{
  "name": "confidante",
  "version": "0.2.2",
  "description": "Share your environment files through password managers",
  "author": "Matteo Rigon @reegodev",
  "bin": {
    "confidante": "./bin/run"
  },
  "homepage": "https://github.com/reegodev/confidante",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "reegodev/confidante",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.14",
    "@types/chai": "^4",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.20",
    "chai": "^4",
    "chai-spies": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "lodash": "^4.17.21",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "prettier": "^2.8.7",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "confidante",
    "dirname": "confidante",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test": "mocha \"test/**/*.test.ts\"",
    "coverage": "nyc --reporter=lcov pnpm test",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/reegodev/confidante/issues"
}
