    "pnpm-lock.yaml"
  ],
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "chalk": "^4.0.0",
    "commander": "^10.0.0",
    "figures": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.20",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "shx": "^0.3.3",
    "stdout-stderr": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1"
  },
  "scripts": {
